;;; AutoCAD help tests.
	(defun acadhlp1 ( / prverr )
		(setq prverr *error*)
		(setq *error* silent_err)
		(setq x '("a" "b" ("c" "d") "b"))
		(setq rc (subst "qq" "b" x))
		(loglist errloc rc)
		(setq rc (subst "qq" "z" x))
		(loglist errloc rc)
		(setq rc (subst "qq" '("c" "d") x))
		(loglist errloc rc)
		(setq rc (subst '("qq" "rr") '("c" "d") x))
		(loglist errloc rc)
		(setq rc (subst '("qq" "rr") "z" x))
		(loglist errloc rc)
		(setq *error* prverr)
	)
	(defun acadhlp2 ( / prverr )
		(setq prverr *error*)
		(setq *error* silent_err)
		(setq x '((first "john") (mid "q") (last "public")))
		(setq xold (assoc 'first x) xnew '(first "j"))
		(setq rc (subst xnew xold x))
		(loglist errloc rc)
		(setq *error* prverr)
	)

