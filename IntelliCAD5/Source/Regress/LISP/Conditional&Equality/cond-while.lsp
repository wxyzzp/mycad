;;; ---------------------------------------------
;;; IntelliCAD help tests.
	(defun icadhlp1 ( / prverr )
		(setq prverr *error*)
		(setq *error* silent_err)
		(setq rc (setq x 25))
		(while (/= x 20)
			(logflt errloc x)
			(setq x (1- x))
		)
		(setq *error* prverr)
	)

;;; ---------------------------------------------
;;; AutoCAD help tests.
	(defun acadhlp1 ( / prverr )
		(setq prverr *error*)
		(setq *error* silent_err)
		(setq rc (setq x 1))
		(while (<= x 10)
			(logflt errloc x)
			(setq x (1+ x))
		)
		(setq *error* prverr)
	)

