	(defun emptystr ( / prverr )
		(setq prverr *error*)
		(setq *error* silent_err)
		(setq rc (setq x ""))
		(while x
			(logstr errloc "hi from while") 
			(setq x nil)
		)
		(setq *error* prverr)
	)
